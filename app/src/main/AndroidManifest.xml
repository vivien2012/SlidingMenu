<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vivien.slidingmenu">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.NormalSlidingMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.QQFiveSlidingMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.AfterQQFiveSlidingMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.DNormalSlidingMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.DQQFiveSlidingMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.DAfterQQFiveSlidingMenuActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>